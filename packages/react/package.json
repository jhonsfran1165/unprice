{
  "name": "@unprice/react",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": ["dist/**"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist",
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@tanstack/react-query": "5.85.6",
    "@unprice/api": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
